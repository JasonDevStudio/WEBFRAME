@using Library.StringItemDict
<!DOCTYPE html>
<!--[if IE 8]><html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]><html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="zh" class="no-js" xmlns="http://www.w3.org/1999/xhtml">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    @RenderSection("metaTags", false)
    
   @if ((Request.Browser.Browser.Equals("IE") || Request.Browser.Browser.Equals("InternetExplorer")) && Convert.ToDecimal(Request.Browser.Version) <= 9)
   {
     <meta  http-equiv ="X-UA-Compatible"  content ="IE=EmulateIE8"   />   
   }
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    
   
    <meta content="后台管理" name="description" />
    <meta content="Jason.Yao" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.css")?@BaseDict.Version" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/font-awesome/css/font-awesome.min.css")?@BaseDict.Version" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Scripts/plugins/uniform/css/uniform.default.min.css")?@BaseDict.Version" rel="stylesheet" type="text/css"/>
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL STYLES --> 
    @RenderSection("Styles", required: false)
    <!-- END PAGE LEVEL STYLES -->
    <!-- BEGIN THEME STYLES -->
    <link href="@Url.Content("~/Content/style-metronic.css")?@BaseDict.Version" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/style.css")?@BaseDict.Version 1" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/style-responsive.css")?@BaseDict.Version" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/plugins.css")?@BaseDict.Version" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/custom.css")?@BaseDict.Version" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/themes/blue_old.css")?@BaseDict.Version" rel="stylesheet" type="text/css" id="style_color"/>
    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="@Url.Content("~/favicon.ico")"/>
</head>
<!-- END HEAD -->
     @RenderBody() 
<!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
<!-- BEGIN CORE PLUGINS -->

<script src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")?@BaseDict.Version" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery-migrate-1.2.1.min.js")?@BaseDict.Version" type="text/javascript"></script>
<!-- IMPORTANT! Load jquery-ui-1.10.1.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
<script src="@Url.Content("~/Scripts/plugins/jquery-ui/jquery-ui-1.10.2.custom.min.js")?@BaseDict.Version" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/plugins/bootstrap/js/bootstrap.min.js")?@BaseDict.Version" type="text/javascript"></script>
<!--[if lt IE 9]><script src="@Url.Content("~/Scripts/plugins/excanvas.min.js")?@BaseDict.Version" type="text/javascript"></script>    <script src="@Url.Content("~/Scripts/plugins/respond.min.js")?@BaseDict.Version" type="text/javascript"></script>  <![endif]-->
<script src="@Url.Content("~/Scripts/plugins/jquery-slimscroll/jquery.slimscroll.min.js")?@BaseDict.Version" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/plugins/jquery.blockui.min.js")?@BaseDict.Version?@BaseDict.Version" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/plugins/jquery.cookie.min.js")" type="text/javascript"></script>
@*<script src="@Url.Content("~/Scripts/bootstrap/jquery.uniform.min.js")?@BaseDict.Version" type="text/javascript"></script>*@

<!-- END CORE PLUGINS -->
<!-- BEGIN PAGE LEVEL PLUGINS -->
<!-- END PAGE LEVEL PLUGINS -->
<!-- BEGIN PAGE LEVEL SCRIPTS -->
<script src="@Url.Content("~/Scripts/app.js")?@BaseDict.Version 12" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/index.js")?@BaseDict.Version 12" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/artDialog/artDialog.js?skin=default")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Common.js")?@BaseDict.Version" type="text/javascript"></script>



<!-- END PAGE LEVEL SCRIPTS -->
<script type="text/javascript">
    jQuery(document).ready(function () {

        App.init(); 

        $(document).keydown(function (event) {
            if (event.keyCode == 13) {
                var btnSubmit = $("#btnSubmit");
                var btnSearch = $("#btnSearch");
                if (btnSubmit) {
                    btnSubmit.trigger("click");
                }

                if (btnSearch) {
                    btnSearch.trigger("click");
                }
            }
        });
    });</script>
<!-- END JAVASCRIPTS -->
<!-- END BODY -->
@RenderSection("Scripts", required: false) 
@Html.Raw(MvcApp.Common.CustomScript.GetCustomScriptContent(this.ViewContext))
</html>
